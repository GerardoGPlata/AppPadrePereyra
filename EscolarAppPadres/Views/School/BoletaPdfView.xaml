<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:syncfusionPdf="clr-namespace:Syncfusion.Maui.PdfViewer;assembly=Syncfusion.Maui.PdfViewer"
             x:Class="EscolarAppPadres.Views.School.BoletaPdfView"
             Title="Boleta">
    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Compartir"
                     Order="Primary"
                     Priority="0"
                     Clicked="OnShareClicked"/>
    </ContentPage.ToolbarItems>
    <syncfusionPdf:SfPdfViewer x:Name="pdfViewer"
                               HorizontalOptions="Fill"
                               VerticalOptions="Fill"
            ShowToolbars="False"/>
</ContentPage>
